
@{
    ViewData["Title"] = "FbList";
}


@section MoreScripts {
    <link href="~/lib/datatables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>

   
    <script>
    $(document).ready(function () {
        $('#jsMovieTable').DataTable({
            ordering: true,
            paging: true,
            searching: true,
            info: true,
            lengthMenu: [[6, 10, 20, -1], [6, 10, 20, "All"]],
        })
    })
    </script>
}

<h2>Feedback Form</h2>

@if (TempData["Message"] != null)
{
    <div class="alert alert-@TempData["MsgType"]">
        @TempData["Message"]
    </div>
}

<table id="jsMovieTable" class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Actions</th>            
        </tr>
    </thead>
    <tbody>
        <tr>

            <td>
                <a>
                    Singapore OpenGov Leadership Forum
                </a>
            </td>

            <td>
                <a asp-controller="Home"
                   asp-action="ViewFB">
                    View
                </a>
                |
                <a asp-controller="Home"
                   asp-action="SendFeedback">
                    Send
                </a>
                
                <nav class="navbar navbar-expand-sm navbar-light bg-light">
                    <button class="navbar-toggler" type="button"
                            data-toggle="collapse" data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item dropdown">
                                <a href="#" class="nav-link dropdown-toggle" id="navbarDropdown"
                                   role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    Chart
                                </a>
                                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating1">Event</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating2">Job Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating3">Accommodation Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating4">Welcome Kit Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating5">Communication emails Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating6">Transportation Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating7">Welcome activity Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating8">Venue Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating9">Activities Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating10">Closing Ceremony Satisfaction</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating11">Welcome activity Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating12">Speaker #1 Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating13">Activity #1 Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating14">Speaker #2 Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating15">Activity #2 Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating16">Closing activity Relevancy</a>
                                    <a class="nav-link" asp-controller="Chart" asp-action="Rating17">Session content Relevancy</a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </nav>

            </td>

        </tr>
         
    </tbody>
</table>






